{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "An app to teach you to write React",
  "main": "src/App.js",
  "scripts": {
    "test": "jest --silent",
    "test:update": "jest --silent -u",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "tslint --project .",
    "dev": "parcel src/index.html",
    "test:watch": "jest --silent --watch",
    "test:coverage": "jest --silent --coverage",
    "build": "parcel build --public-url ./dist/ src/index.html",
    "start": "npm run build && babel-node ./src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btholt/complete-intro-to-react-v4.git"
  },
  "author": "Brian Holt <btholt@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/btholt/complete-intro-to-react-v4/issues"
  },
  "homepage": "https://github.com/btholt/complete-intro-to-react-v4#readme",
  "devDependencies": {
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2",
    "react-test-renderer": "^16.7.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@reach/router": "^1.1.1",
    "@types/node": "^10.12.18",
    "emotion": "^10.0.5",
    "express": "^4.16.4",
    "petfinder-client": "^0.0.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^10.0.0",
    "react-loadable": "^5.5.0"
  }
}
